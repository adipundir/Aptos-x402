{
  "name": "aptos-x402",
  "version": "2.0.0",
  "description": "HTTP 402 Payment Protocol SDK for Aptos - Axios-compatible payment wrapper with automatic network detection and zero fallbacks",
  "author": "Aditya Pundir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adipundir/aptos-x402"
  },
  "homepage": "https://aptos-x402.vercel.app",
  "keywords": [
    "x402",
    "aptos",
    "payment",
    "micropayment",
    "http-402",
    "blockchain",
    "cryptocurrency",
    "axios",
    "axios-compatible",
    "typescript",
    "nextjs",
    "middleware",
    "ai-payments",
    "machine-to-machine"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run build:sdk && npm run build:demo",
    "build:sdk": "tsc -p tsconfig.sdk.json",
    "build:demo": "next build --turbopack",
    "start": "next start",
    "demo": "tsx scripts/demo-agent.ts",
    "test:payment": "tsx scripts/test-payment-flow.ts",
    "prepublishOnly": "npm run build:sdk",
    "test": "echo \"Tests coming soon\" && exit 0",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "peerDependencies": {
    "@aptos-labs/ts-sdk": ">=1.26.0",
    "next": ">=15.0.0"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": ">=1.26.0",
    "@auth/drizzle-adapter": "^1.11.1",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@langchain/core": "^0.3.0",
    "@langchain/google-genai": "^0.1.0",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@shikijs/transformers": "^3.14.0",
    "@splinetool/react-spline": "^4.1.0",
    "@splinetool/runtime": "^1.12.6",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "got": "^14.6.5",
    "langchain": "^0.3.0",
    "lucide-react": "^0.548.0",
    "next": ">=15.0.0",
    "next-auth": "^5.0.0-beta.30",
    "openai": "^6.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.4.2",
    "shiki": "^3.14.0",
    "tailwind-merge": "^3.3.1",
    "x402-axios": "^0.6.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "tailwindcss": "^4",
    "tsx": "^4.7.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
